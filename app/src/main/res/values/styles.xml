<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="ThemeOverlay.AppCompat.Light">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="customPopupStyle">
        <item name="overlapAnchor">false</item>
    </style>

    <style name="TestingTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="DarkMaybe" parent="Theme.AppCompat.NoActionBar"></style>

</resources>
